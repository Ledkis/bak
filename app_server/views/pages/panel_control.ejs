<div class="panel panel-default">
  <div class="panel-heading">
   Control Panel
 </div>
 <!-- /.panel-heading -->
 <div class="panel-body">
  <form role="form" id="controlForm" action="/" method="get">
    <div class="form-group">
      <label>Selects</label>
      <select class="form-control" name="selectDataId">
        <% dataInfo.forEach(function(dataId) { %>
        <option
          <% if(wikiDatas.dataId === dataId) { %>
            selected
          <% } %>    
        ><%= dataId %></option>
        <% }) %>
      </select>
    </div>
    <div class="form-group">
      <label>Radio Buttons</label>
      <% dataFrom.forEach(function(dataFrom) { %>
      <div class="radio">
        <label>
          <input type="radio" name="optionFrom" id="option<%= dataFrom %>" value="<%= dataFrom %>"
          <% if(dataFrom === from) { %>
            checked
          <% } %>
          ><%= dataFrom %>
        </label>
      </div>
      <% }) %>
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
  </form>


</div>
<!-- /.panel-body -->
</div>